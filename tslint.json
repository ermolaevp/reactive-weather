{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-a11y"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": true,
    "no-unused-expression": true,
    "no-console": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "src/serviceWorker.js"]
  }
}
